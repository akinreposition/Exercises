
public class Asterisks {
	
	public void calculate() {
		for(int i=1; i<=10; i++) {
			for (int j = 0; j < i; j++) {
			System.out.print("*");
		}
			System.out.println(" ");
		}
		System.out.println();
		for(int i=10; i>=1; i--) {
			for (int j = 0; j < i; j++) {
			System.out.print(" *");
		}
			System.out.println(" ");
		}
	}
		
}
