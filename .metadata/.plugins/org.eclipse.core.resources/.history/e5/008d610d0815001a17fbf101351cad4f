package assignment9;

public class Quadrillateral {

		private Points a;
		private Points b;
		private Points c;
		private Points d;
		
		
		
		public Quadrillateral(Points a, Points b, Points c, Points d) {
			super();
			this.a = a;
			this.b = b;
			this.c = c;
			this.d = d;
		}
		public Points getA() {
			return a;
		}
		public void setA(Points a) {
			this.a = a;
		}
		public Points getB() {
			return b;
		}
		public void setB(Points b) {
			this.b = b;
		}
		public Points getC() {
			return c;
		}
		public void setC(Points c) {
			this.c = c;
		}
		public Points getD() {
			return d;
		}
		public void setD(Points d) {
			this.d = d;
		}
		
		
}
